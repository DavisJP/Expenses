<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/exp_frag_content"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.opentab.ExpenseFragment">
    <android.support.v7.widget.RecyclerView android:id="@+id/exp_list"
        android:name="com.davismiyashiro.expenses.fragments.ExpenseFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        tools:context=".view.opentab.ExpenseFragment"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:listitem="@layout/list_item_expense" />

</android.support.design.widget.CoordinatorLayout>